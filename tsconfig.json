{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "declaration": true,
        "outDir": "./lib",
        "target": "es2017"
    },
    "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "typings/browser",
        "typings/browser.d.ts",
        "./lib"
    ],
    "types": [
        "node", "es2015"
    ]
}
